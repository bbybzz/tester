    function generateItem(category) {
      const data = category === 'cards' ? cardData : category === 'accounts' ? accountData : category === 'malware' ? malwareData : fullzData;
      const item = data[Math.floor(Math.random() * data.length)];
      const cardNumber = item.bin + Array(10).fill(0).map(() => Math.floor(Math.random() * 10)).join('');
      const expiryMonth = String(Math.floor(Math.random() * 12) + 1).padStart(2, '0');
      const expiryYear = String(Math.floor(Math.random() * 10) + 24);
      const cvv = category === 'cards' ? String(Math.floor(Math.random() * 1000)).padStart(3, '0') : '';
      const price = Math.floor(Math.random() * 76 + 25); // Prices from $25 to $100
      const soldOut = item.soldOut || Math.random() < 0.2; // 20% chance of being sold out unless specified
      const review = item.reviews[Math.floor(Math.random() * item.reviews.length)];
      return {
        cardNumber: cardNumber.match(/.{1,4}/g)?.join('-') || cardNumber,
        expiry: category === 'cards' ? `${expiryMonth}/${expiryYear}` : '',
        cvv,
        cardType: item.type,
        logo: item.logo,
        price,
        use: item.use,
        soldOut,
        rating: review.rating,
        comment: review.comment
      };
    }

    function displayItems(filter = 'all', containerId = 'cardContainer', category = 'cards') {
      const container = document.getElementById(containerId);
      container.innerHTML = '';
      const items = Array(1000).fill().map(() => generateItem(category)); // Thousands of items
      let filteredItems = items;
      if (filter !== 'all') {
        filteredItems = items.filter(item => item.cardType === filter);
      }
      filteredItems.slice(0, 50).forEach(item => addItem(item, container, category)); // Display 50 for performance
      updateSidebar();
    }

    function addItem(item, container, category) {
      const itemDiv = document.createElement('div');
      itemDiv.className = 'item';
      itemDiv.innerHTML = `
        ${item.logo && !item.soldOut ? `<img src="${item.logo}" alt="${item.cardType}">` : '<p class="sold-out">SOLD OUT</p>'}
        <h3>${item.cardType} - $${item.price * 50}</h3>
        <p>Price: $${item.price}</p>
        <p>${category === 'cards' ? `Card Number: ${item.cardNumber.slice(0, 5)}XXXX-XXXX-XXXX` : `ID: ${item.cardNumber}`}</p>
        ${item.expiry ? `<p>Expiry: ${item.expiry} | CVV: ***</p>` : ''}
        <p>${item.use}</p>
        <p class="stars">${'★'.repeat(Math.floor(item.rating))}${item.rating % 1 ? '☆' : ''} ${item.rating}/5</p>
        <p class="comment">${item.comment}</p>
        <button class="buy-btn" ${item.soldOut ? 'disabled' : ''} onclick="buyItem('${item.cardType} - ${item.cardNumber}')">${item.soldOut ? 'Sold Out' : 'Contact to Purchase'}</button>
      `;
      container.appendChild(itemDiv);
    }

    function updateSidebar() {
      const sidebarMenu = document.getElementById('sidebar-menu');
      const categories = [
        { name: 'Credit Cards', id: 'cards', container: 'cardContainer' },
        { name: 'Store Accounts', id: 'accounts', container: 'accountContainer' },
        { name: 'Malware', id: 'malware', container: 'malwareContainer' },
        { name: 'Fullz', id: 'fullz', container: 'fullzContainer' }
      ];
      sidebarMenu.innerHTML = '';
      categories.forEach(category => {
        const types = (category.id === 'cards' ? cardData : category.id === 'accounts' ? accountData : category.id === 'malware' ? malwareData : fullzData)
          .map(item => item.type);
        types.forEach(type => {
          const count = Array.from(document.getElementById(category.container).children)
            .filter(item => item.querySelector('h3').textContent.includes(type) && !item.querySelector('.sold-out')).length;
          const li = document.createElement('li');
          li.textContent = `${type} (${count} available)`;
          li.className = count === 0 ? 'sold-out' : '';
          li.onclick = count > 0 ? () => filterItems(type, category.container) : null;
          sidebarMenu.appendChild(li);
        });
      });
    }

    function searchItems(query) {
      const containers = ['cardContainer', 'accountContainer', 'malwareContainer', 'fullzContainer'];
      containers.forEach(containerId => {
        const container = document.getElementById(containerId);
        const items = container.getElementsByClassName('item');
        Array.from(items).forEach(item => {
          const title = item.querySelector('h3').textContent.toLowerCase();
          item.style.display = title.includes(query.toLowerCase()) ? 'block' : 'none';
        });
      });
      updateSidebar();
    }

    function filterItems(type, containerId) {
      const category = containerId === 'cardContainer' ? 'cards' : containerId === 'accountContainer' ? 'accounts' : containerId === 'malwareContainer' ? 'malware' : 'fullz';
      displayItems(type, containerId, category);
    }

    function sortItems(containerId) {
      const container = document.getElementById(containerId);
      const items = Array.from(container.children);
      items.sort((a, b) => {
        const priceA = parseInt(a.querySelector('p:nth-child(3)').textContent.split('$')[1]);
        const priceB = parseInt(b.querySelector('p:nth-child(3)').textContent.split('$')[1]);
        return priceA - priceB;
      });
      container.innerHTML = '';
      items.forEach(item => container.appendChild(item));
      updateSidebar();
    }

    function buyItem(item) {
      const itemData = generateItem('cards');
      fetch('YOUR_WEBHOOK_URL', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ item, data: itemData, timestamp: new Date().toISOString() })
      });
      alert('DM @RakRunnin on Telegram (https://t.me/rakrunning) to pay via Cash App!');
      const firstItem = document.querySelector('.item');
      if (firstItem) {
        firstItem.querySelector('p:nth-child(4)').textContent = `${itemData.cardNumber.slice(0, 5)}XXXX-XXXX-XXXX`;
        if (itemData.expiry) firstItem.querySelector('p:nth-child(5)').textContent = `Expiry: ${itemData.expiry} | CVV: ***`;
      }
    }

    function loadPage(sectionId) {
      document.getElementById('loading-screen').style.display = 'flex';
      setTimeout(() => {
        document.getElementById('cards-section').style.display = sectionId === 'cards' ? 'block' : 'none';
        document.getElementById('accounts-section').style.display = sectionId === 'accounts' ? 'block' : 'none';
        document.getElementById('malware-section').style.display = sectionId === 'malware' ? 'block' : 'none';
        document.getElementById('fullz-section').style.display = sectionId === 'fullz' ? 'block' : 'none';
        if (sectionId === 'cards') displayItems('all', 'cardContainer', 'cards');
        else if (sectionId === 'accounts') displayItems('all', 'accountContainer', 'accounts');
        else if (sectionId === 'malware') displayItems('all', 'malwareContainer', 'malware');
        else if (sectionId === 'fullz') displayItems('all', 'fullzContainer', 'fullz');
        document.getElementById('loading-screen').style.display = 'none';
      }, 1500);
    }

    function verifyCaptcha() {
      const input = document.getElementById('captcha-input').value;
      if (input === '11') {
        document.getElementById('captcha-screen').style.display = 'none';
      } else {
        alert('Incorrect. Try again.');
      }
    }

    window.onload = () => {
      displayItems('all', 'cardContainer', 'cards');
      displayItems('all', 'accountContainer', 'accounts');
      displayItems('all', 'malwareContainer', 'malware');
      displayItems('all', 'fullzContainer', 'fullz');
    };
  </script>
</body>
</html>
